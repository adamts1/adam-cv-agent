    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Debug setupCareerVector.ts",
            "runtimeArgs": [
                "--loader",
                "ts-node/esm",
                "--project",
                "tsconfig-esm.json"
            ],
            "cwd": "${workspaceFolder}",
            "envFile": "${workspaceFolder}/.env",
            "console": "integratedTerminal",
            "skipFiles": ["<node_internals>/**"],
            "program": "${workspaceFolder}/src/loaders/setupCareerVector.ts"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Current file",
            "runtimeArgs": [
                "--loader",
                "ts-node/esm",
                "--project",
                "tsconfig-esm.json"
            ],
            "cwd": "${workspaceFolder}",
            "args": [
                "${relativeFile}"
            ],
            "envFile": "${workspaceFolder}/.env",
            "console": "integratedTerminal",
            "skipFiles": ["<node_internals>/**"]
        }
    ]
}